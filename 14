#include <stdio.h>
#include <malloc.h>
void fill(int **a, int n){
    int nm = 1, sloy;

    for(sloy = 0; sloy < n / 2; sloy++){

        for(int i = sloy; i < n - sloy; i++)
            a[sloy][i] = nm++;

        for(int i = sloy + 1; i < n - sloy; i++)
            a[i][n - sloy - 1] = nm++;

        for(int i = n- sloy - 2; i >= sloy; i--)
            a[n - sloy -1][i] = nm++;

        for (int i = n - sloy - 2; i > sloy; i--)
            a[i][sloy] = nm++;
    }
    if (n % 2 == 1)
        a[n / 2][n / 2] = nm;

    for (int i = 0; i < n;i++){
        for (int j = 0; j < n; j++)
            printf("%d\t", a[i][j]);
        puts("\n");
    }

}
int main(){
    int n, **a;
    scanf_s("%d", &n);
    a = (int **)malloc(n*sizeof(int*));

    for (int i = 0; i < n; i++)
        a[i] = (int *) malloc(n*sizeof(int));

    fill(a, n);
    
    free(a);
    return 0 ;
}
