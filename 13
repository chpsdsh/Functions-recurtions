#include <stdio.h>
#include <stdlib.h>
#include <string.h>
#include <math.h>
int vchislo(int *a, int n){
    int nom = 0;
    for(int i = n; i>=0; i--)
        nom += *(a+i) * pow(10, n - i) ;
    return nom;
}
int* conv(char* num){
    int len =(int)strlen(num);
    int* res = (int*) malloc((len)*sizeof(int));
    for(int  i = 0; i< len; i++)
        *(res + i) = *(num + i) - 48;
    return  res;
}

int multiply(int x, int *res, int res_size)
{
    int carry = 0;

    for (int i=0; i<res_size; i++)
    {
        int prod = *(res + i) * x + carry;

        *(res + i) = prod % 10;

        carry  = prod/10;
    }

    while (carry)
    {
        *(res + res_size) = carry%10;
        carry = carry/10;
        res_size++;
    }
    return res_size;
}

void factorial(int n)
{
    int *res = (int*)(malloc(5000*sizeof(int)));

    *res = 1;
    int res_size = 1;

    for (int x = 2; x <= n; x++)
        res_size = multiply(x, res, res_size);

    for (int i = res_size-1; i>=0; i--)
        printf ("%d",*(res + i ));
    free(res);
}

int main()
{
    char *a =(char*)malloc(sizeof(char)*500);
    scanf_s("%499s",a, 500);
    int  *inta = conv(a);
    int n = vchislo(inta, strlen(a)-1);
    factorial(n);
    free(a);
    free(inta);

    return 0;
}
