#include <stdio.h>
#include <malloc.h>

int slozhenie(int *a, int *b, int MxLen) {
    int *c;
    
    c = (int*)malloc((MxLen+1)*sizeof(int));
    for(int i = 0; i < (MxLen + 1); i++)
        *(c+i) = 0;
    for(int i = 0; i < (MxLen); i++){
        *(c + MxLen - i - 1) = 
    }
}

int main() {
    int n1, n2, *a, *b, MxLen, Nachalo;

    scanf_s("%d", &n1);
    scanf_s("%d", &n2);
    if (n1 >= n2){
        MxLen = n1;
        Nachalo = n2 - 1; 
    }
    else
    {
        MxLen = n2;
        Nachalo = n1 - 1;
    }      
    a = (int*)(malloc(MxLen * sizeof(int)));
    b = (int*)(malloc(MxLen * sizeof(int)));
    for(int i = 0; i < MxLen; i++){
        scanf_s("%d", a + i);
        if(i >= Nachalo)
            scanf_s("%d", b + i);
        else
            *(b+i) = 0;
    }
        

    scanf_s("%d", &n2);
    for(int i = 0; i < n2; i++)
        scanf_s("%d", b + i);

    if (n1>n2)
        for(int i = 0; i < n1; i++)
        printf("%d", (slozhenie(a, n1, b, n2)+i));
    if (n2>n1)
        for(int i = 0; i < n2; i++)
            printf("%d", (slozhenie(a, n1, b, n2)+i));
    free(a);
    free(b);
    return 0;
}
