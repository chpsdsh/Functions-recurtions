#include <stdio.h>

int SummaCifr(int n){
    if (n < 10)
        return n;
    else
        return  n % 10 + SummaCifr(n / 10);
}

int CisKor(int n){
    if (SummaCifr(n) < 10)
        return SummaCifr(n);
    else
        return CisKor(SummaCifr(n));
}

int main() {
    int n;
    scanf_s("%d", &n);
    printf("%d", CisKor(n));
    return 0;

}
