#include <stdio.h>
#include <malloc.h>
#include <stdlib.h>

void zap(int *x, int n){
    for(int i=0; i < n; i++){
        *(x+i) = rand();
    }
}

int main() {
    int *x, *y, n, new;
    scanf_s("%d" ,&n);
    x = (int*)malloc(n * sizeof(int));

    zap(x ,n);

    for(int i=0; i < n; i++)
        printf("%d\n",*(x+i));

    return 0;
}
