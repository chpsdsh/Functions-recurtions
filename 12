#include <stdio.h>
#include <malloc.h>

void slozhenie(int *a, int *b, int MxLen) {
    int res;
    for (int i = MxLen; i > 0; i--) {
        res = *(a + i) + *(b + i);
        if (res > 9) {
            *(a + i) = res % 10;
            *(a + i - 1) += res / 10;
        } else
            *(a + i) = res % 10;
    }
    int i;
    if(*a == 0)
        i = 1;
    else
        i = 0;

    for (i; i < MxLen; i++)
        printf("%d", *(a+i));
}

int main() {
    int n1, n2, *a, *b, MxLen, Nachalo, fl =0;

    scanf_s("%d", &n1);
    scanf_s("%d", &n2);

    if (n1 >= n2){
        MxLen = n1 + 1;
        Nachalo = n1 - n2 +1;
        fl = 0;
    }
    else
    {
        MxLen = n2 + 1;
        Nachalo = n2 - n1 + 1;
        fl = 1;
    }

    a=(int*) malloc(MxLen*sizeof (int));
    b=(int*) malloc(MxLen*sizeof (int));
    *a = 0;
    *b = 0;
    printf("vvod a\n");
    for(int i = 1; i < MxLen; i++)
        if(fl == 0)
            scanf_s("%d", a + i);
        else if (i >= Nachalo)
            scanf_s("%d", a + i);
        else
            *(a+i) = 0;

    printf("vvod b\n");

    for(int i = 1; i < MxLen; i++)
        if(fl == 1)
            scanf_s("%d", b + i);
        else if (i >= Nachalo)
            scanf_s("%d", b + i);
        else
            *(b+i) = 0;

    slozhenie(a,b,MxLen);
    free(a);
    free(b);
    return 0;
}
